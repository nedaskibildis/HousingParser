///////////////////////////////////////////////////////////////////////////////////////////////////////////
// Provides KnockOut binding definitions for custom bindings that are specific to the Planning module.
// This includes bindings for the calendar, accordions, tabs, etc.
///////////////////////////////////////////////////////////////////////////////////////////////////////////


// custom binding for fullCalendar to display as a weekly schedule
ko.bindingHandlers.planningCalendar = {
    options: {},
    calendar: {},
    init: function (element, valueAccessor, allBindingsAccessor) {
        // in this case, value is set to the currentTerm
        var value = ko.utils.unwrapObservable(valueAccessor());

        if (typeof value === 'function') value = value();

        //default to 8am and then see if we can refine this based on the planned sections
        var firstHour = 8;
        //find the lowest hour in all the calendar events. If one of the hour is less than 8am then use it otherwise default will be 8am
        try {
            for (var i = 0; i < value.length; i++) {
                var eventStartHour = value[i].start.getHours();
                firstHour = eventStartHour <= firstHour ? eventStartHour - 1 : firstHour;
            }
        }
        catch (e) {
            firstHour = 8;
        }

        var currentDate = new Date();
        currentDate.setHours(firstHour);
        currentDate.setMinutes(0);
        currentDate.setSeconds(0);

        var tab = allBindingsAccessor().planningCalendarActiveTab ? allBindingsAccessor().planningCalendarActiveTab() || {} : {};
        var clickHandler = allBindingsAccessor().planningCalendarClickHandler || {};
        var detailsClickHandler = allBindingsAccessor().planningCalendarSectionDetailsClickHandler || {};
        var height = allBindingsAccessor().planningCalendarScheduleHeight || null;
        var date = new Date();
        calendar = new FullCalendar.Calendar(element, {
            headerToolbar: {left:'', center:'', right:''},
            initialView: 'timeGridWeek',
            editable: false,
            allDaySlot: false,
            slotEventOverlap:false,
            eventTextColor: "black",
            weekends: true,
            dayHeaderFormat: { weekday: 'short' },
            nowIndicator: false,
            slotMinTime: currentDate.toLocaleTimeString(),
            height: height,
            events:value,
            eventContent: function(arg){
                 var html = "";
                 html += "<div class='fc-event-main-frame' data-instrMethod='" + arg.event.extendedProps.instrMethod + "'>";
                   //  "<div class='fc-event-time'>"; 
                     html +=  "<div class='fc-event-title-container'>";
                 var titleString = arg.event.title + " " + arg.event.extendedProps.instrMethod;
                 if (!arg.event.extendedProps.showEventTitleAsLink) {
                     html += "<div class='fc-event-title fc-sticky'>" + escapeHtml( titleString ) + "</div>";
                 } else {
                     html += "<div class='fc-event-title'><a href='javascript:void(0)' class='fc-event-title-link fc-event-section-details'>" + escapeHtml( titleString )+ "</a>";
                        
                 }
                 
                 if (arg.event.extendedProps.allowRemove) {

                     html += "<div class='calendar-remove-event'>" +
                         "<a role='presentation' href='javascript:void(0)' ><span class='esg-icon__container'><svg class='esg-icon esg-icon--neutral esg-icon--xsmall'><use xlink:href='#icon-close'></use></svg></span></a>" +
                         "</div>"
                 }

                 html += "</div></div></div>";
                 return { html: html };
             },
            eventDidMount: function (info) {

                var removeButton = info.el.getElementsByClassName('calendar-remove-event')[0];
                if (removeButton) {
                    $(removeButton).click(function () {
                        clickHandler(info);
                    });
                    $(removeButton).attr("title", "Remove " + info.event.title);
                    $(removeButton).attr("id", info.event.id);
                }

                var eventTitle = info.el.getElementsByClassName('fc-event-title-link')[0];
                if (eventTitle) {
                    $(eventTitle).click(function () {
                        detailsClickHandler(info);
                    });
                    if (info.event.extendedProps.ariaLabel) {
                        $(eventTitle).attr("aria-label", info.event.extendedProps.ariaLabel);
                        $(eventTitle).attr("title", info.event.title);
                        if (info.event.extendedProps.idAttribute) {
                            $(eventTitle).attr("id", (info.event.extendedProps.idAttribute));
                        }
                        else {
                            $(eventTitle).attr("id", info.event.id);
                        }
                    }
                }
             }
        });
        calendar.render();
        calendar.updateSize();
        calendar.off('eventClick');
    },
    update: function (element, valueAccessor, allBindingsAccessor) {
        var render = ko.computed({
            read: function () {
                var i = 0;
                // in this case, value is set to the currentTerm
                var value = ko.utils.unwrapObservable(valueAccessor());

                if (typeof value === 'function') value = value();

                //this is when an existing event is updated like background color change on hover
                var tab = allBindingsAccessor().planningCalendarActiveTab ? allBindingsAccessor().planningCalendarActiveTab() || {} : {};
                var eventToUpdate = allBindingsAccessor().calendarEventToUpdate ? allBindingsAccessor().calendarEventToUpdate() : null;
                var calendarAvaialbleSectionsEvents = allBindingsAccessor().calendarAvaialbleSectionsEvents ? allBindingsAccessor().calendarAvaialbleSectionsEvents() : null;
                calendar.batchRendering(function () {
                    //remove all existing events
                    var allEvents = calendar.getEvents();
                    if (allEvents != null && allEvents.length > 0) {
                        allEvents.forEach(function (event) {
                            event.remove();
                        });
                    }

                    //add all the events again
                    if (value.length > 0){
                        for (i=0; i < value.length; i++) {
                            calendar.addEvent(value[i]);
                        }
                    }

                    //add available sections if any
                    if (calendarAvaialbleSectionsEvents && calendarAvaialbleSectionsEvents.length > 0) {
                        for (i = 0; i < calendarAvaialbleSectionsEvents.length; i++) {
                            calendar.addEvent(calendarAvaialbleSectionsEvents[i]);
                        }
                    }

                    //changing the style of available sections events
                    if (eventToUpdate !== undefined && eventToUpdate !== null ) {
                        if (allEvents != null && allEvents.length > 0) {
                            var eventById = calendar.getEventById(eventToUpdate.eventId);
                            if (eventById != null) {
                                eventById.setProp('classNames', eventToUpdate.classNames);
                            }
                        }
                    }
                });

            },
            disposeWhenNodeIsRemoved: element
        }).extend({ rateLimit: 500 });
    }
};
;
/////////////////////////////////////////////////////////////////////////////////////////////
//  This file contains utility functionality that needs to be made available across
//  two or more views within the Student area of Self Service. Functions placed into this
//  file should be as succint and context-less as possible. These functions will be available
//  in every view in the Student area, and loaded in the Student _ViewStart page. Do NOT place
//  anything into this file that needs to be available in other Areas, such as Finance.
/////////////////////////////////////////////////////////////////////////////////////////////

// Determine the value to display for a given Section's credits amount
function courseOrSectionCreditsCeusDisplay(item) {
    var display = "";
    if (item != null) {
        display = ((item.MaximumCredits != null) && item.MaximumCredits > 0 && item.MaximumCredits !== item.MinimumCredits) ? item.MinimumCredits + " to " + item.MaximumCredits : item.MinimumCredits;
        if (display === null) {
            if (item.Ceus != null) {
                display = item.Ceus + " CEUs";
            }
        }
    }
    return display;
}

// Determine if the given course or section item represents a variable credit item
function isCourseOrSectionVariableCredit(item) {
    return (item != null && item.MinimumCredits != null && item.MaximumCredits != null &&
        item.MinimumCredits != item.MaximumCredits);
}

// Time of day validation common logic
function checkValidTimeFormat(timeExact) {

        var amTimeSuffix = "am";
        var pmTimeSuffix = "pm";
        var timeValue = timeExact.toLowerCase().replace(amTimeSuffix, '').replace(/ /g, '')
                       .replace(pmTimeSuffix, '').replace(':', '').replace('.', '');
        
        //check if timeValue is not a number
        if (isNaN(timeValue)) { return null; }

        var timeExactValue = null;        
        //check valid values with length of number supplied
        switch (timeValue.length) {
                case 1:
                case 2:
                        timeExactValue = moment(timeExact, "hA").format("hh:mm A");
                        break;
                case 3:
                    var newTimeExact = timeValue[0] + '.' + timeValue[1] + timeValue[2] + (timeExact.toLowerCase().indexOf(pmTimeSuffix) > 0 ? pmTimeSuffix : amTimeSuffix);
                        timeExactValue = moment(newTimeExact, "h.mmA").format("hh:mm A");
                        break;
                case 4:
                        timeExactValue = moment(timeExact, "hhmmA").format("hh:mm A");
                        break;
        }            
        if (timeExactValue == "Invalid date") {
            return null;
        }
        return timeExactValue;
};
/*
* jQuery outerHTML
*
* Copyright (c) 2008 Ca-Phun Ung <caphun at yelotofu dot com>
* Dual licensed under the MIT (MIT-LICENSE.txt)
* and GPL (GPL-LICENSE.txt) licenses.
*
* http://yelotofu.com/labs/jquery/snippets/outerhtml/
*
* outerHTML is based on the outerHTML work done by Brandon Aaron
* But adds the ability to replace an element.
*/

(function ($) {
	$.fn.outerHTML = function (s) {
		return (s)
			? this.before(s).remove()
			: $('<p>').append(this.eq(0).clone()).html();
	}
})(jQuery);;
function ExpandAllAccordions() {
    $(".expand-collapse-all").text("Collapse All");
    $(".accordion-region, .accordion-region-opened").each(function () {
        var activeAccordion = $(this).accordion('option', 'active');
        // Collapse accordions have activeAccordion == 'false'
        if (activeAccordion !== 0) {
            $(this).accordion('activate', 0);
        }
    });
    $(this).trigger("focus");
}

function CollapseAllAccordions() {
    $(".expand-collapse-all").text("Expand All");
    $(".accordion-region, .accordion-region-opened").each(function () {
        var activeAccordion = $(this).accordion('option', 'active');
        // Expanded accordions have activeAccordion == '0'
        if (activeAccordion === 0) {
            $(this).accordion('activate', 0);
        }
    });
    $(this).trigger("focus");
}

/* Restoration of deprecated .toggle(function, function) 
 * Courtesy https://forum.jquery.com/topic/beginner-function-toggle-deprecated-what-to-use-instead */
$.fn.clicktoggle = function (a, b) {
    return this.each(function () {
        var clicked = false;
        $(this).on("click", function () {
            if (clicked) {
                clicked = false;
                return b.apply(this, arguments);
            }
            clicked = true;
            return a.apply(this, arguments);
        });
    });
};

$(document).ready(function () {
    if ($(".accordion-region").length > 0) {
        $(".accordion-region").accordion(
            {
                active: false,
                collapsible: true,
                autoHeight: false,
                clearStyle: true,
                icons: { 'header': 'ui-icon-carat-1-e', 'headerSelected': 'ui-icon-carat-1-s' }
            }
        );
    }
    if ($(".accordion-region-opened").length > 0) {
        $(".accordion-region-opened").accordion(
            {
                collapsible: true,
                autoHeight: false,
                clearStyle: true,
                icons: { 'header': 'ui-icon-carat-1-e', 'headerSelected': 'ui-icon-carat-1-s' }
            }
        );
    }
    if ($(".accordion-fixed").length > 0) {
        $(".accordion-fixed").accordion(
            {
                disabled: true,
                active: 0,
                collapsible: false,
                autoHeight: false,
                clearStyle: true,
                icons: false
            }
        );
    }

    if ($(".accordion-region-opened").length > 0 && $(".accordion-region").length === 0) {
        ExpandAllAccordions();
        $(".expand-collapse-all").clicktoggle(CollapseAllAccordions, ExpandAllAccordions);
    } else {
        $(".expand-collapse-all").clicktoggle(ExpandAllAccordions, CollapseAllAccordions);
    }
});;
